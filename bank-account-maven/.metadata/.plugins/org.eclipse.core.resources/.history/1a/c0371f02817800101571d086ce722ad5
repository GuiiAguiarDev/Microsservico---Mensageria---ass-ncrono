package com.bank.messasing;

import org.springframework.amqp.rabbit.annotation.RabbitListener;

import com.bank.repository.CurrentAccountRepository;

public class CurrentAccountConsumer {
	
	private final CurrentAccountRepository accountRepository;

	public CurrentAccountConsumer(CurrentAccountRepository accountRepository) {

		this.accountRepository = accountRepository;
	}
	
	
	@RabbitListener(queues = RabbitMQConfig.QUEUE_CURRENT_ACCOUNT)
	
}
